{
    "name": "romain-jacquart/phpunit-snapshots",
    "description": "Snaphot testing for PHPUnit",
    "license": "MIT",
    "keywords": ["phpunit", "snapshot"],
    "authors": [
        {
            "name": "Madewithlove",
            "email": "heroes@madewithlove.be"
        }
    ],
    "autoload": {
        "psr-4": {
            "Madewithlove\\PhpunitSnapshots\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Madewithlove\\PhpunitSnapshots\\": "tests"
        }
    },
    "require": {},
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0",
        "phpunit/phpunit": "^7.5",
        "symfony/var-dumper": "4.3"
    },
    "scripts": {
        "test": "phpunit",
        "lint": "php-cs-fixer fix"
    },
    "config": {
        "sort-packages": true
    }
}
